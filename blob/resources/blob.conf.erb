{
  "drivers" : [
    {
        "fs-<%= provisioned_service.name %>": {
            "type": "fs",
            "option": {
                "root": "<%= blob_dir %>",
                "node_exepath": "<%= @nodejs_path %>",
                "gc_exepath": "<%= @blobd_path %>/drivers/fs/fs_gc.js",
                "gcfc_exepath": "<%= @blobd_path %>/drivers/fs/fs_gcfc.js",
                "gctmp_exepath": "<%= @blobd_path %>/drivers/fs/fs_gctmp.js",
                "ec_exepath": "<%= @blobd_path %>/drivers/fs/fs_ec.js",
                "quota": 2147483648,
                "obj_limit": 32768
            }
        }
    }
    ],
  "port" : <%= blob_port %> ,
  "current_driver" : "fs-<%= provisioned_service.name %>",
  "logtype" : "winston",
  "logfile" : "<%= log_file %>",
  "keyID" : "<%= keyid %>",
  "secretID" : "<%= secretid %>",
  "auth" : "<%= @blobd_auth %>",
  "debug" : false,
  "account_file" : "<%= account_file %>",
  "account_api" : true
}
