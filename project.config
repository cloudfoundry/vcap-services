[receive]
	requireChangeId = true
[access "refs/*"]
	owner = group Administrators
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group Services Committers
	label-Verified = -1..+1 group Services Team
[access "refs/tags/*"]
	pushTag = group Services Committers
	forgeAuthor = group Administrators
	forgeAuthor = group Services Committers
	forgeCommitter = group Administrators
	forgeCommitter = group Services Committers
[access "refs/for/*"]
	pushMerge = group Services Team
[project]
	state = hidden
